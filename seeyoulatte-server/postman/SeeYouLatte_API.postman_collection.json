	"item": [
		{
			"name": "Auth",
			"item": [
				{
					"name": "Sign Up",
					"event": [
						{
							"listen": "test",
							"script": {
								"type": "text/javascript",
								"exec": [
									"// Extract token from response",
									"if (pm.response.code === 200 || pm.response.code === 201) {",
									"    const responseData = pm.response.json();",
									"    if (responseData.token) {",
									"        pm.environment.set('jwt_token', responseData.token);",
									"        console.log('Token saved to environment:', responseData.token);",
									"    }",
									"    if (responseData.user && responseData.user.id) {",
									"        pm.environment.set('user_id', responseData.user.id);",
									"        console.log('User ID saved:', responseData.user.id);",
									"    }",
									"}"
								]
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json",
								"type": "text"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n    \"email\": \"test@example.com\",\n    \"password\": \"password123\",\n    \"name\": \"Test User\"\n}"
						},
						"url": {
							"raw": "{{base_url}}/api/auth/signup",
							"host": ["{{base_url}}"],
							"path": ["api", "auth", "signup"]
						}
					}
				},
